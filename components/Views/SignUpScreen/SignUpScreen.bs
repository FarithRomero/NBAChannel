sub init()
    bindVariables()
    bindStyles()
end sub

sub bindVariables()
    m.title = m.top.findNode("title")
    m.tyC = m.top.findNode("tyC")
end sub

sub bindStyles()
    m.title.text = SignUpScreen.TITTLE
    m.tyC.text = SignUpScreen.TYC
    stopLoad()
end sub

sub stopLoad()
    m.top.loadComplete = true
end sub

